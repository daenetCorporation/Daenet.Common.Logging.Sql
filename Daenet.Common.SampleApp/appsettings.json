{
  "Logging": {
    "IncludeScopes": true,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "SqlProvider": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "SqlProviderSettings": {
      "ConnectionString": "",
      "TableName": "SqlLog",
      "CreateTblIfNotExist": true,
      "IncludeExceptionStackTrace": false,
      "IgnoreLoggingErrors": false,
      "ScopeColumnMapping": {
        "RequestId": "RequestId",
        "SCOPEPATH":  "Scope",
      }
    }
  }
}
